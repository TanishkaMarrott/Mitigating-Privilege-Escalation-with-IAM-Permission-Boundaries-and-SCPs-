{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Deny",
        "Action": [
          "iam:CreateUser",
          "iam:CreateRole",
          "iam:AttachUserPolicy",
          "iam:AttachRolePolicy",
          "iam:PutUserPolicy",
          "iam:PutRolePolicy"
        ],
        "Resource": "*",
        "Condition": {
          "ArnNotEquals": {
            "aws:PrincipalArn": "arn:aws:iam::123456789012:role/ProjectLeadRole"
          }
        }
      }
    ]
  }
  